<?xml version="1.0" encoding="utf-8"?>

<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.ftwtour">

    <application
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:icon="@mipmap/ic_launcher"
        android:theme="@style/AppTheme"
        android:allowBackup="true">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DivesActivity"
            android:label="@string/category_dives" />

        <activity
            android:name=".ParksActivity"
            android:label="@string/category_parks" />
        <activity
            android:name=".CoffeeActivity"
            android:label="@string/category_coffee" />
        <activity
            android:name=".DessertsActivity"
            android:label="@string/category_desserts" />
    </application>

</manifest>
    
